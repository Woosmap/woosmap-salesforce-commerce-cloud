<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="WOOSMAP_Public_Key">
                <display-name xml:lang="x-default">Woosmap Public Key</display-name>
                <description xml:lang="x-default">This is the public key used for connection to your Woosmap Account
                </description>
                <type>string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value></default-value>
            </attribute-definition>
            <attribute-definition attribute-id="WOOSMAP_Mobile_Breakpoint">
                <display-name xml:lang="x-default">Woosmap Webapp Mobile Breakpoint</display-name>
                <description xml:lang="x-default">This is the breakpoint in pixel where the webapp is render in Mobile
                </description>
                <type>int</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value>0</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="WOOSMAP_Webapp_Container_style">
                <display-name xml:lang="x-default">Woosmap Webapp Container Style</display-name>
                <description xml:lang="x-default">This is the inline style for the Webapp Container
                </description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value>width:100%; height:600px</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="GOOGLE_Client_Id">
                <display-name xml:lang="x-default">Google Client ID</display-name>
                <description xml:lang="x-default">This is the Google Client ID to identify your requests to Google APIs
                </description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value></default-value>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="WOOSMAP_Preferences">
                <display-name xml:lang="x-default">Woosmap General Preferences</display-name>
                <attribute attribute-id="WOOSMAP_Public_Key"/>
                <attribute attribute-id="WOOSMAP_Mobile_Breakpoint"/>
                <attribute attribute-id="WOOSMAP_Webapp_Container_style"/>
                <attribute attribute-id="GOOGLE_Client_Id"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
</metadata>
