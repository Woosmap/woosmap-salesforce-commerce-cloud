{
  "name": "int_woosmap",
  "version": "22.1.0",
  "description": "Woosmap SFRA Link Cartridge",
  "scripts": {
    "test": "sgmf-scripts --test test/unit/**/*.js",
    "test:integration": "sgmf-scripts --integration 'test/integration/**/*.js'",
    "compile:scss": "sgmf-scripts --compile css",
    "compile:js": "sgmf-scripts --compile js",
    "compile:fonts": "node bin/Makefile compileFonts",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "sgmf-scripts --lint css",
    "lint:js": "sgmf-scripts --lint js",
    "build": "npm run compile:js && npm run compile:scss",
    "upload": "sgmf-scripts --upload",
    "uploadCartridge": "sgmf-scripts --uploadCartridge cartridges/int_woosmap",
    "watch": "sgmf-scripts --watch",
    "watch:static": "sgmf-scripts --watch static"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:SalesforceCommerceCloud/link_woosmap.git"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-dollar-sign": "~1.0.2",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "~26.1.4",
    "eslint-plugin-jquery": "~1.5.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-promise": "~6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-sitegenesis": "~1.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "~6.12.0",
    "proxyquire": "2.1.3",
    "sgmf-scripts": "^2.4.1",
    "stylelint": "^10.0.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^4.2.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^7.3.1",
    "css-loader": "^0.28.11"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "babel": {
    "presets": [
      "env"
    ]
  },
  "packageName": "int_woosmap",
  "paths": {
    "base": "../storefront-reference-architecture/cartridges/app_storefront_base/"
  }
}
